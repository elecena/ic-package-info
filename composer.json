{
    "name": "elecena/ic-package-info",
    "description": "Extract IC package from a given string",
    "keywords": ["parser", "electronic-components", "electronics-engineering", "integrated-circuits", "elecena"],
    "type": "library",
    "require": {
      "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "10.x"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Brencz",
            "email": "macbre@elecena.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Elecena\\Utils\\": "src/Elecena/Utils/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "coverage": [
            "XDEBUG_MODE=coverage phpunit --coverage-html=.coverage --coverage-clover=.coverage.xml --coverage-text"
        ]
  }
}
